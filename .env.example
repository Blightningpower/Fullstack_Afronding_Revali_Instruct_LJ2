# Database instellingen
MSSQL_HOST=localhost
MSSQL_PORT=1433
MSSQL_DATABASE=revali_db

# SQL Server SA login (alleen voor lokale ontwikkeling)

# Vervang door je eigen SA wachtwoord
MSSQL_SA_PASSWORD=jouw_sa_wachtwoord_hier

# SQL Server applicatie-specifieke login
MSSQL_USER=revali_login
MSSQL_PASSWORD=jouw_applicatie_wachtwoord_hier

# Beveiliging

# Genereer een sterke geheime sleutel voor JWT
JWT_SECRET=jouw_zeer_geheime_jwt_sleutel_hier